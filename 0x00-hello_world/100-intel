#include <stdbool.h>
#include<string.h>
bool is_merge(const char *s, const char *part1, const char *part2) {
    if (*s == '\0') {
        return *part1 == '\0' && *part2 == '\0';
    }

    if (*part1 == *s) {
        if (is_merge(s + 1, part1 + 1, part2)) {
            return true;
        }
    }

    if (*part2 == *s) {
        if (is_merge(s + 1, part1, part2 + 1)) {
            return true;
        }
    }

    return false;
}
